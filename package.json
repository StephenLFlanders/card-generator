{
  "name": "card-generator-server",
  "version": "1.0.0",
  "description": "Local server for testing the playing card generator",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx tsc -w --project tsconfig.json",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:build": "tsc --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "rimraf": "^5.0.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "card-generator",
    "express",
    "server",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
